<!DOCTYPE html>
<html>
<head>
<title>Isaac's Baseball Tryouts Validated Form</title>
<link rel="stylesheet" href="style2.css">
<script>

function doClear()
{
	//First and Last Name
	document.BaseballForm.first.value = "";
	document.BaseballForm.first.style.backgroundColor="#e7e9f2";
	document.BaseballForm.last.value = "";
	document.BaseballForm.last.style.backgroundColor="#e7e9f2";
	//Email
	document.BaseballForm.email.value = "";
	document.BaseballForm.email.style.backgroundColor="#e7e9f2";
	//Phone Number
	document.BaseballForm.phone.value = "";
	document.BaseballForm.phone.style.backgroundColor="#e7e9f2";
	//Birthday
	document.BaseballForm.month.value = "";
	document.BaseballForm.month.style.backgroundColor="#e7e9f2";
	document.BaseballForm.day.value = "";
	document.BaseballForm.day.style.backgroundColor="#e7e9f2";
	document.BaseballForm.year.value = "";
	document.BaseballForm.year.style.backgroundColor="#e7e9f2";
	
	//Height and Weight
	document.BaseballForm.heightFT.value = "";
	document.BaseballForm.heightFT.style.backgroundColor="#e7e9f2";
	document.BaseballForm.heightIN.value = "";
	document.BaseballForm.heightIN.style.backgroundColor="#e7e9f2";
	document.BaseballForm.weight.value = "";
	document.BaseballForm.weight.style.backgroundColor="#e7e9f2";
	
	//Password
	document.BaseballForm.password.value = "";
	document.BaseballForm.password.style.backgroundColor="#e7e9f2";
	
	//Throwing Hand
	document.BaseballForm.hand[0].checked = false;
	document.BaseballForm.hand[1].checked = false;	
	
	//Batting Side
	document.BaseballForm.bats[0].checked = false;
	document.BaseballForm.bats[1].checked = false;
	document.BaseballForm.bats[2].checked = false;

	//Position Trying out For
	document.BaseballForm.position[0].checked = false;
	document.BaseballForm.position[1].checked = false;
	document.BaseballForm.position[2].checked = false;
	document.BaseballForm.position[3].checked = false;
	document.BaseballForm.position[4].checked = false;
	document.BaseballForm.position[5].checked = false;
	document.BaseballForm.position[6].checked = false;
	document.BaseballForm.position[7].checked = false;
	document.BaseballForm.position[8].checked = false;
	document.BaseballForm.position[9].checked = false;
	
	
	
	
	
	//Uniform Mailing Address
	document.BaseballForm.st.value = "";
	document.BaseballForm.st.style.backgroundColor="#e7e9f2";
	document.BaseballForm.city.value = "";
	document.BaseballForm.city.style.backgroundColor="#e7e9f2";
	document.BaseballForm.state.value = "";
	document.BaseballForm.state.style.backgroundColor="#e7e9f2";
	document.BaseballForm.zip.value = "";
	document.BaseballForm.zip.style.backgroundColor="#e7e9f2";

	//Comments
	document.BaseballForm.comments.value = "";
	return;
	

}



function doSubmit()
{
	//First Name Validation (ALSO CHANGES BORDER COLOR RED WHEN WRONG USER INPUT OCCURS)	
	{
		var first=document.BaseballForm.first.value;
		if(first==0)
	{
		alert("Please enter your first name");
		document.BaseballForm.first.style.backgroundColor= "#ffcccb";
		document.BaseballForm.first.focus();
		return false;
	}
	//Makes sure user can't type in any numbers as a Name
	if(!isNaN(first))
	{
		alert("Please Enter in only Characters for your first name");
		document.BaseballForm.first.style.backgroundColor="#ffcccb";
		document.BaseballForm.first.focus();
		return false;
	}
	//Makes sure user input is 5 to 15 characters long
	if ((first.length<5) || (first.length>15))
	{
		alert("Your first name can be up to 5-15 Characters long");
		document.BaseballForm.first.style.backgroundColor="#ffcccb";
		document.BaseballForm.first.focus();
		return false;
	}			
	else{document.BaseballForm.first.style.backgroundColor="#e7e9f2";}
	}
	//END
	
	//LAST NAME VALIDATION
	{
		var last=document.BaseballForm.last.value;
		if(last==0)
	{
		alert("Please enter your last name");
		document.BaseballForm.last.style.backgroundColor="#ffcccb";
		document.BaseballForm.last.focus();
		return false;
	}
	//Makes sure user can't type in any numbers as a Name
	if(!isNaN(last))
	{
		alert("Please Enter Only Characters for your Last name");
		document.BaseballForm.last.style.backgroundColor="#ffcccb";
		document.BaseballForm.last.focus();
		return false;
	}
	//Makes sure user input is 5 to 15 characters long
	if ((last.length<5) || (last.length>15))
	{
		alert("Your last name can be up to 5-15 Characters long");
		document.BaseballForm.last.style.backgroundColor="#ffcccb";
		document.BaseballForm.last.focus();
		return false;
	}			
	else{document.BaseballForm.last.style.backgroundColor="#e7e9f2";}
	}
	//END
	
	//EMAIL VALIATION
	{
		var email=document.BaseballForm.email.value;
		if(email==0)
	{
		alert("Please enter in your Email");
		document.BaseballForm.email.style.backgroundColor="#ffcccb";
		document.BaseballForm.email.focus();
		return false;
	}
	//Makes sure user input is between 6 to 30 characters long
	if ((email.length<6) || (email.length>40))
	{
		alert("Sorry your email must be between 6 to 40 characters long");
		document.BaseballForm.email.style.backgroundColor="#ffcccb";
		document.BaseballForm.email.focus();
		return false;
	}			
	else{document.BaseballForm.email.style.backgroundColor="#e7e9f2";}
	}
	//END
	
	//PHONE NUMBER VALIATION
	{
		var phone=document.BaseballForm.phone.value;
		if(phone==0)
	{
		alert("Please enter in your Phone Number");
		document.BaseballForm.phone.style.backgroundColor="#ffcccb";
		document.BaseballForm.phone.focus();
		return false;
	}
	//Makes sure user can't type in any letters as a Phone Number
	if (isNaN(phone))
	{
		alert("Enter a valid Phoner Number (Like: 8186588916)");
		document.BaseballForm.phone.style.backgroundColor="#ffcccb";
		document.BaseballForm.phone.focus();
		return false;
	}
	//Makes sure phone number is 10 characters long
	if ((phone.length<10))
	{
		alert("Your phone number must be up to 10 digits long");
		document.BaseballForm.phone.style.backgroundColor="#ffcccb";
		document.BaseballForm.phone.focus();
		return false;
	}	
		else{document.BaseballForm.phone.style.backgroundColor="#e7e9f2";}	
	}
	//END
	

	
	//BIRTH MONTH VALIDATION
	if (validateBirthMonth()==false)
	{
		alert("Please select your Birth Month");
		document.BaseballForm.month.style.backgroundColor="#ffcccb";
		document.BaseballForm.month.focus();
		return;
	}
	else{document.BaseballForm.month.style.backgroundColor="#e7e9f2";}	
	
	//END
	
	
	
	//DAY OF BIRTH VALIDATION
	if (validateDay()==false)
	{
		alert("Please select your day of birth");
		document.BaseballForm.day.style.backgroundColor="#ffcccb";
		document.BaseballForm.day.focus();
		return;
	}
	else{document.BaseballForm.day.style.backgroundColor="#e7e9f2";}	
	//END
	
	
	
	
	//BIRTH YEAR VALIATION
	{
        var year=document.BaseballForm.year.value;
		if(year==0)
	{
		alert("Please enter your Birth Year");
		document.BaseballForm.year.style.backgroundColor="#ffcccb";
		document.BaseballForm.year.focus();
		return false;
	}
	//Makes sure user can't type in any letters as a year
	if (isNaN(year))
	{
		alert("Enter a valid year like 2003 ");
		document.BaseballForm.year.style.backgroundColor="#ffcccb";
		document.BaseballForm.year.focus();
		return false;
	}
	if(year.length<4)
	{
		alert("Invalid amount of numbers for Birth Year (Example: 2003)");
		document.BaseballForm.year.style.backgroundColor="#ffcccb";
		document.BaseballForm.year.focus();
		return false;
	}
	else{document.BaseballForm.year.style.backgroundColor="#e7e9f2";}	
    }
	//END
	
	
	//HEIGHT IN FEET VALIATION
	{
		var heightFT=document.BaseballForm.heightFT.value;
		if (heightFT==0)
	{
		alert("Please enter your height in feet ");
		document.BaseballForm.heightFT.style.backgroundColor="#ffcccb";
		document.BaseballForm.heightFT.focus();
		return false;
	}
	//Makes sure user can't type in any letters as their height in feet
	if (isNaN(heightFT))
	{
		alert("Please enter only one number for feet in Height");
		document.BaseballForm.heightFT.style.backgroundColor="#ffcccb";
		document.BaseballForm.heightFT.focus();
		return false;
	}
		else{document.BaseballForm.heightFT.style.backgroundColor="#e7e9f2";}	
	}
	//END
	
	
	//HEIGHT IN INCHES VALIATION
	{
		var heightIN=document.BaseballForm.heightIN.value;
		if (heightIN==0)
	{
		alert("Please enter in your height in inches ");
		document.BaseballForm.heightIN.style.backgroundColor="#ffcccb";
		document.BaseballForm.heightIN.focus();
		return false;
	}
	//Makes sure user can't type in any letters as their height
	if (isNaN(heightIN))
	{
		alert("Please enter in only numbers for inches in height");
		document.BaseballForm.heightIN.style.backgroundColor="#ffcccb";
		document.BaseballForm.heightIN.focus();
		return false;
	}
		else{document.BaseballForm.heightIN.style.backgroundColor="#e7e9f2";}
	}
	//END
	
	
	//WEIGHT VALIATION
	{
		var weight=document.BaseballForm.weight.value;
		if (weight==0)
	{
		alert("Please enter your weight in 'Height and Weight'");
		document.BaseballForm.weight.style.backgroundColor="#ffcccb";
		document.BaseballForm.weight.focus();
		return false;
	}
	//Makes sure user can't type in any letters as their weight
	if (isNaN(weight))
	{
		alert("Please enter only numbers for weight ");
		document.BaseballForm.weight.style.backgroundColor="#ffcccb";
		document.BaseballForm.weight.focus();
		return false;
	}
	if(weight.length<2)
	{
		alert("Invalid amount of numbers for weight (Example: 215) ");
		document.BaseballForm.weight.style.backgroundColor="#ffcccb";
		document.BaseballForm.weight.focus();
		return false;
	}
		else{document.BaseballForm.weight.style.backgroundColor="#e7e9f2";}
	}
	//END
	
	//THROWING HAND VALIATION
	if (validateThrowingHand() == false)
	{
		alert("Throwing hand information required");
		return;
	}
	//END
	
	//BATTING SIDE VALIATION
	if (validateBats() == false)
	{
		alert("Batting side required");
		return;
	}
	//END
	
	//POSITION VALIATION
	if (validatePositions() == false)
	{
		alert("Please select at least one position you would like to try out for. If you can't decide select the option, 'Any' ");
		return;
	}
	//END
	
	//STREET ADDRESS VALIATION
	{
		var st=document.BaseballForm.st.value;
		if (st==0)
	{
		alert("Please enter in your Street Address");
		document.BaseballForm.st.style.backgroundColor="#ffcccb";
		document.BaseballForm.st.focus();
		return false;
	}
	//Makes sure address is no greater than 40 characters long
	if  ((st.length>40))
	{
		alert("Sorry your address must be no greater than 40 characters long");
		document.BaseballForm.st.style.backgroundColor="#ffcccb";
		document.BaseballForm.st.focus();
		return false;
	}
	//Makes sure street address is between 20 to 46 characters long
	if ((st.length<20) || (st.length>46))
	{
		alert("Sorry your street address must be between 20 to 46 characters long");
		document.BaseballForm.st.style.backgroundColor="#ffcccb";
		document.BaseballForm.st.focus();
		return false;
	}			
		else{document.BaseballForm.st.style.backgroundColor="#e7e9f2";}
	}
	//END
	
	//CITY VALIATION
	{
		var city=document.BaseballForm.city.value;
		if (city==0)
	{
		alert("Please enter in your City");
		document.BaseballForm.city.style.backgroundColor="#ffcccb";
		document.BaseballForm.city.focus();
		return false;
	}
	//Makes sure user can't type in any numbers as their city
	if(!isNaN(city))
	{
		alert("Please Enter in only Characters for your city");
		document.BaseballForm.city.style.backgroundColor="#ffcccb";
		document.BaseballForm.city.focus();
		return false;
	}
	//Makes sure that city name is at least 3 characters long
	if  ((city.length<3))
	{
		alert("Sorry your city name must be at least 3 characters long");
		document.BaseballForm.city.style.backgroundColor="#ffcccb";
		document.BaseballForm.city.focus();
		return false;
	}
		else{document.BaseballForm.city.style.backgroundColor="#e7e9f2";}
	}
	//END
	
	
	
	//STATE VALIATION
	if (validateState()==false)
	{
		alert("Please select your state");
		document.BaseballForm.state.style.backgroundColor="#ffcccb";
		document.BaseballForm.state.focus();
		return;
	}
	else{document.BaseballForm.state.style.backgroundColor="#e7e9f2";}
	//END
	
	
	//ZIP CODE VALIATION
	{
		var zip=document.BaseballForm.zip.value;
		if (zip==0)
	{
		alert("Please enter in your zip code");
		document.BaseballForm.zip.style.backgroundColor="#ffcccb";
		document.BaseballForm.zip.focus();
		return false;
	}
	//Makes sure user can't type in any letters as their zip code
	if (isNaN(zip))
	{
		alert("Please enter in only numbers for your zip code ");
		document.BaseballForm.zip.style.backgroundColor="#ffcccb";
		document.BaseballForm.zip.focus();
		return false;
	}
	//Makes sure that zip code is 5 digits long
	if  ((zip.length<5))
	{
		alert("Sorry your zip code must be 5 digits long");
		document.BaseballForm.zip.style.backgroundColor="#ffcccb";
		document.BaseballForm.zip.focus();
		return false;
	}
	
		else{document.BaseballForm.zip.style.backgroundColor="#e7e9f2";}
	}
	//END
	
	
	
	//PASSWORD VALIATION
	{
		var password=document.BaseballForm.password.value;
		if (password==0)
	{
		alert("Password creation required");
		document.BaseballForm.password.style.backgroundColor="#ffcccb";
		document.BaseballForm.password.focus();
		return false;
	}
	//Makes sure password is between 6 to 20 characters long
	if ((password.length<6) || (password.length>20))
	{
		alert("Sorry your password must be between 6 to 20 characters long");
		document.BaseballForm.password.style.backgroundColor="#ffcccb";
		document.BaseballForm.password.focus();
		return false;
	}		
		else{document.BaseballForm.password.style.backgroundColor="#e7e9f2";}
	}
	
	alert("Your information has been successfully submitted."+"\n"+"Remember to NOT forget your password before you go to enter it in at the front desk."+"\n"+"Thank you :)");
	return;
}


function validateBirthMonth()
{
		if(document.BaseballForm.month.value=="blank") return false;
		return true;
}

function validateDay()
{
		if(document.BaseballForm.day.value=="blank") return false;
		return true;
		
}

function validateThrowingHand()
{
	if (document.BaseballForm.hand[0].checked) return true;
	if (document.BaseballForm.hand[1].checked) return true;
	return false;
}

function validateBats()
{
	if (document.BaseballForm.bats[0].checked) return true;
	if (document.BaseballForm.bats[1].checked) return true;
	if (document.BaseballForm.bats[2].checked) return true;
	return false;
}

function validatePositions()
{
	if (document.BaseballForm.position[0].checked) return true;
	if (document.BaseballForm.position[1].checked) return true;
	if (document.BaseballForm.position[2].checked) return true;
	if (document.BaseballForm.position[3].checked) return true;
	if (document.BaseballForm.position[4].checked) return true;
	if (document.BaseballForm.position[5].checked) return true;
	if (document.BaseballForm.position[6].checked) return true;
	if (document.BaseballForm.position[7].checked) return true;
	if (document.BaseballForm.position[8].checked) return true;
	if (document.BaseballForm.position[9].checked) return true;
	return false;
}



function validateState()
{
		if(document.BaseballForm.state.value=="blank") return false;
		return true;
}

</script>
</head>

